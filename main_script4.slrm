#!/bin/bash -l
#SBATCH --time=10:00:00
#SBATCH --mem=2G
#SBATCH -o num_sims-job-%a.out
#SBATCH --array=1-10
module load matlab

path=pwd
interm_folder='interm'
out_folder='out_folder'

TT=50

matlab -nojvm -r "compute_num_sims('$path', '$interm_folder','$out_folder', $SLURM_ARRAY_TASK_ID, $TT); quit"
